{
  "name": "grapefruitlab",
  "private": true,
  "version": "1.0.0",
  "description": "a website for the Denver theater collective",
  "main": "package.json",
  "author": "Miriam Suzanne <miriam@oddbird.net>",
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "build": "sanity build",
    "watch:sass": "sass --watch content/sass:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass content/sass:_site/css",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "deploy": "npm-run-all build:sass build:eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@sanity/base": "^2.27.0",
    "@sanity/core": "^2.27.0",
    "@sanity/default-layout": "^2.27.0",
    "@sanity/default-login": "^2.27.0",
    "@sanity/desk-tool": "^2.27.0",
    "@sanity/eslint-config-studio": "^2.0.0",
    "@sanity/vision": "^2.27.0",
    "accoutrement": "^3.0.1",
    "cssremedy": "^0.1.0-beta.2",
    "eslint": "^8.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7",
    "react": "^17.0",
    "react-dom": "^17.0",
    "sass": "^1.49.7",
    "styled-components": "^5.2.0"
  }
}
